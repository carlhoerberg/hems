# TYPE acload_frequency gauge
acload_frequency <%=acload.frequency%>

# TYPE acload_current gauge
# TYPE acload_voltage gauge
# TYPE acload_active_power gauge
# TYPE acload_reactive_power gauge
# TYPE acload_apparent_power gauge
# TYPE acload_power_factor gauge
# TYPE acload_day_consumed_energy counter
<%[1, 2, 3].each do |l|%>
acload_current{L="<%=l%>"} <%=acload.current(l)%>
acload_voltage{L="<%=l%>"} <%=acload.voltage(l)%>
acload_active_power{L="<%=l%>"} <%=acload.active_power(l)%>
acload_reactive_power{L="<%=l%>"} <%=acload.reactive_power(l)%>
acload_apparent_power{L="<%=l%>"} <%=acload.apparent_power(l)%>
acload_power_factor{L="<%=l%>"} <%=acload.power_factor(l)%>
acload_day_consumed_energy{L="<%=l%>"} <%=acload.day_consumed_energy(l)%>
<%end%>
