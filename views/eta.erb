# TYPE eta_pellets_in_storage gauge
eta_pellets_in_storage <%=eta.pellets%>

# TYPE eta_stallet_temp gauge
eta_stallet_temp <%=eta.stallet_temp%>

# TYPE eta_stallet_should_temp gauge
eta_stallet_should_temp <%=eta.stallet_should_temp%>

# TYPE eta_stallet_pump untyped
eta_stallet_pump <%=eta.stallet_pump%>

# TYPE eta_vvx_primary_temp gauge
eta_vvx_primary_temp <%=eta.vvx_primary_temp%>

# TYPE eta_vvx_primary_should_temp gauge
eta_vvx_primary_should_temp <%=eta.vvx_primary_should_temp%>

# TYPE eta_vvx_primary_return_temp gauge
eta_vvx_primary_return_temp <%=eta.vvx_primary_return_temp%>

# TYPE eta_vvx_secondary_temp gauge
eta_vvx_secondary_temp <%=eta.vvx_secondary_temp%>

# TYPE eta_outdoor_temp gauge
eta_outdoor_temp <%=eta.outdoor_temp%>

# TYPE eta_boiler_temp gauge
# TYPE eta_boiler_return_temp gauge
# TYPE eta_boiler_pump_speed gauge
# TYPE eta_boiler_total_consumption counter
# TYPE eta_boiler_flue_gas_fan_rpm gauge
<%[1, 2].each do |b|%>
eta_boiler_temp{boiler="<%=b%>"} <%=eta.boiler_temp(b)%>
eta_boiler_return_temp{boiler="<%=b%>"} <%=eta.boiler_return_temp(b)%>
eta_boiler_pump_speed{boiler="<%=b%>"} <%=eta.boiler_pump_speed(b)%>
eta_boiler_total_consumption{boiler="<%=b%>"} <%=eta.boiler_total_consumption(b)%>
eta_boiler_flue_gas_fan_rpm{boiler="<%=b%>"} <%=eta.boiler_flue_gas_fan_rpm(b)%>
<%end%>

# TYPE eta_tank_temp gauge
<%[1,2,3,4,5].each do |sensor|%>
eta_tank_temp{sensor="<%=sensor%>"} <%=eta.tank_temp(sensor)%>
<% end %>

# TYPE eta_tank_should_temp gauge
eta_tank_should_temp <%=eta.tank_should_temp%>

# TYPE eta_tank_return_temp gauge
eta_tank_return_temp <%=eta.tank_return_temp%>
